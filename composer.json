{
    "name": "meilunzhi/dingtalk",
    "description": "EasyDingTalk, Alibaba Dingtalk SDK for PHP",
    "keywords": [
        "dingtalk",
        "dingding",
        "easydingtalk",
        "sdk",
        "钉钉"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "轮子哥哥",
            "email": "meilunzhi@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "overtrue/http": "1.2.3",
        "pimple/pimple": "^3.0",
        "psr/simple-cache": "^3.0",
        "monolog/monolog": "^2.0",
        "symfony/cache": "^6.0",
        "symfony/http-foundation": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "EasyDingTalk\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EasyDingTalk\\Tests\\": "tests/"
        }
    }
}
